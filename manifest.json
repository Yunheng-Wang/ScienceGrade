{
    
    "manifest_version": 3, 

    "name": "ScienceGrade",

    "description": "*",

    "version": "1.0",

    "author": "Yunheng-Wang",
    
    "action": {

      "default_popup": "popup/popup.html",


      "default_icon": {
        "16" : "image/icon.png",
        "32" : "image/icon.png",
        "64" : "image/icon.png",
        "128" : "image/icon.png"
      }
    },

    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://webofscience.clarivate.cn/wos/alldb/summary/*"
        ]
      }
    ]

  }